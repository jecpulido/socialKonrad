<p id="notice"><%= notice %></p>

<h1>Relacions</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Usuario2</th>
      <th>Isvnombre</th>
      <th>Isvapellido</th>
      <th>Isvfechanac</th>
      <th>Isvtelefono</th>
      <th>Isvcorreo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relacions.each do |relacion| %>
      <tr>
        <td><%= relacion.Usuario %></td>
        <td><%= relacion.usuario2 %></td>
        <td><%= relacion.isVNombre %></td>
        <td><%= relacion.isVApellido %></td>
        <td><%= relacion.isVFechaNac %></td>
        <td><%= relacion.isVTelefono %></td>
        <td><%= relacion.isVCorreo %></td>
        <td><%= link_to 'Show', relacion %></td>
        <td><%= link_to 'Edit', edit_relacion_path(relacion) %></td>
        <td><%= link_to 'Destroy', relacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relacion', new_relacion_path %>
