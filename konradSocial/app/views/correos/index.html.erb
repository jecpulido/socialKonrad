<p id="notice"><%= notice %></p>

<h1>Correos</h1>

<table>
  <thead>
    <tr>
      <th>Correo</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @correos.each do |correo| %>
      <tr>
        <td><%= correo.correo %></td>
        <td><%= correo.Usuario %></td>
        <td><%= link_to 'Show', correo %></td>
        <td><%= link_to 'Edit', edit_correo_path(correo) %></td>
        <td><%= link_to 'Destroy', correo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Correo', new_correo_path %>
